        "issue": {
          "id": 1034328540,
          "node_id": "I_kwDOGQLKps49ppnc",
          "url": "https://api.github.com/repos/duckdogg123/-let_em_C-/issues/8",
          "repository_url": "https://api.github.com/repos/duckdogg123/-let_em_C-",
          "labels_url": "https://api.github.com/repos/duckdogg123/-let_em_C-/issues/8/labels%7B/name%7D",
          "comments_url": "https://api.github.com/repos/duckdogg123/-let_em_C-/issues/8/comments",
