    "maintainer_can_modify": true,
    "title": "Don't wipe coins cache when full and instead evict LRU clean entries",
    "user": {
      "login": "andrewtoth",
      "id": 237213,
      "node_id": "MDQ6VXNlcjIzNzIxMw==",
      "avatar_url": "https://avatars.githubusercontent.com/u/237213?v=4",
      "gravatar_id": "",
