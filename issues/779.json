          "repository_url": "https://api.github.com/repos/gridcoin-community/Gridcoin-Research",
          "labels_url": "https://api.github.com/repos/gridcoin-community/Gridcoin-Research/issues/1282/labels%7B/name%7D",
          "comments_url": "https://api.github.com/repos/gridcoin-community/Gridcoin-Research/issues/1282/comments",
