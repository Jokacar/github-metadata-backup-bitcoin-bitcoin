      "source": {
        "issue": {
          "id": 609346220,
          "node_id": "MDExOlB1bGxSZXF1ZXN0NDEwOTQyODYy",
          "url": "https://api.github.com/repos/bitcoin/bitcoin/issues/18820",
          "repository_url": "https://api.github.com/repos/bitcoin/bitcoin",
          "labels_url": "https://api.github.com/repos/bitcoin/bitcoin/issues/18820/labels%7B/name%7D",
          "comments_url": "https://api.github.com/repos/bitcoin/bitcoin/issues/18820/comments",
