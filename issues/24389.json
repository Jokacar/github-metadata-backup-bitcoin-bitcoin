          "url": "https://api.github.com/repos/bitcoin/bitcoin/issues/24392",
          "repository_url": "https://api.github.com/repos/bitcoin/bitcoin",
          "labels_url": "https://api.github.com/repos/bitcoin/bitcoin/issues/24392/labels%7B/name%7D",
