      "tree": {
        "sha": "d7e8871bd50071ecb5be51d394810c825fb0d584",
        "url": "https://api.github.com/repos/bitcoin/bitcoin/git/trees/d7e8871bd50071ecb5be51d394810c825fb0d584"
