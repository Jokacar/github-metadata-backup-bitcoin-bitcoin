      "source": {
        "issue": {
          "id": 43154683,
          "node_id": "MDU6SXNzdWU0MzE1NDY4Mw==",
          "url": "https://api.github.com/repos/mastercoin-MSC/mastercore/issues/131",
          "repository_url": "https://api.github.com/repos/mastercoin-MSC/mastercore",
          "labels_url": "https://api.github.com/repos/mastercoin-MSC/mastercore/issues/131/labels%7B/name%7D",
          "comments_url": "https://api.github.com/repos/mastercoin-MSC/mastercore/issues/131/comments",
