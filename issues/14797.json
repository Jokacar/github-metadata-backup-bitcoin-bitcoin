      "source": {
        "issue": {
          "id": 467968782,
          "node_id": "MDExOlB1bGxSZXF1ZXN0Mjk3NDkxMjU5",
          "url": "https://api.github.com/repos/bitcoin/bitcoin/issues/16392",
          "repository_url": "https://api.github.com/repos/bitcoin/bitcoin",
          "labels_url": "https://api.github.com/repos/bitcoin/bitcoin/issues/16392/labels%7B/name%7D",
          "comments_url": "https://api.github.com/repos/bitcoin/bitcoin/issues/16392/comments",
