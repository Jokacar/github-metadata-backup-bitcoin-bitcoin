      "diff_hunk": "@@ -238,6 +245,36 @@ fs:: namespace, which has unsafe filesystem functions marked as deleted.\n     }\n }\n \n+fn lint_doc_release_note_snippets() -> LintResult {\n+    let files =\n+        check_output(git().args([\"ls-files\", \"--\", \"doc/release-notes/release-notes-*.md\"]))?\n+            .lines()\n+            .filter(|file| {\n+                let num = file\n+                    .trim_start_matches(\"doc/release-notes/release-notes-\")\n+                    .trim_end_matches(\".md\");\n+                // If the number is missing a dot, it is a pull request. Thus, the file is a\n+                // release note snippet.\n+                !num.contains(\".\")\n+            })\n+            .map(|file| file.to_string())\n+            .collect::<Vec<_>>();\n+    if files.is_empty() {\n+        Ok(())\n+    } else {\n+        Err(format!(\n+            r#\"\n+{}\n+^^^\n+Release note snippets must be put into the doc/ folder directly.\n+\n+The doc/release-notes/ folder is for archived release notes of previous releases only.\n+            \"#,\n+            files.join(\"\\n\")\n+        ))\n+    }\n+}",
