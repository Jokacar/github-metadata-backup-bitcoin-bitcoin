      "source": {
        "issue": {
          "id": 1125316383,
          "node_id": "PR_kwDOABII584yIv4N",
          "url": "https://api.github.com/repos/bitcoin/bitcoin/issues/24279",
          "repository_url": "https://api.github.com/repos/bitcoin/bitcoin",
          "labels_url": "https://api.github.com/repos/bitcoin/bitcoin/issues/24279/labels%7B/name%7D",
          "comments_url": "https://api.github.com/repos/bitcoin/bitcoin/issues/24279/comments",
