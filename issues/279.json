          "repository_url": "https://api.github.com/repos/zcash/zcash",
          "labels_url": "https://api.github.com/repos/zcash/zcash/issues/4021/labels%7B/name%7D",
          "comments_url": "https://api.github.com/repos/zcash/zcash/issues/4021/comments",
